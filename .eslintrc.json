{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jquery": true
    },
    "rules": {
      "strict": 0,
      "quotes": ["error", "single", { "allowTemplateLiterals": true }],
      "prefer-arrow-callback": ["warn", { "allowNamedFunctions": true }]
     }
}
